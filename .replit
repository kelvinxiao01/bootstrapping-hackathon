modules = ["nodejs-20", "web", "python-3.11"]
[agent]
expertMode = true
integrations = ["python_openai_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Next.js Dev Server"

[[workflows.workflow]]
name = "Next.js Dev Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd frontend/bootstrapping-hackathon && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 38965
externalPort = 3000

[[ports]]
localPort = 39861
externalPort = 3002

[[ports]]
localPort = 40787
externalPort = 3001

[deployment]
deploymentTarget = "autoscale"
run = ["bash", "-c", "cd frontend/bootstrapping-hackathon && npm run start"]
build = ["bash", "-c", "cd frontend/bootstrapping-hackathon && npm run build"]
